config_version=4

[application]

config/name="Voiceless"
run/main_scene="res://Scenes/menu.tscn"
boot_splash/image="res://sprites/menu/splashscreen.png"
config/icon="res://sprites/Characters/Karu/spr_victory.png"

[audio]

output_latency.web=15

[autoload]

character_choser="*res://Scenes/CharacterSelector.tscn"
Textbox="*res://objects/Textbox.tscn"
ParticlesCache="*res://Scripts/ParticlesCache.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=960
window/size/height=540
window/size/test_width=960
window/size/test_height=540
window/vsync/use_vsync=false
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

